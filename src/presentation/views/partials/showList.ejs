<div class="table-container">
    <link rel="stylesheet" href="/partials/showList/list.css">
    <link href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/5.15.1/css/all.min.css" rel="stylesheet">
    <table class="warehouse-table">
        <thead>
            <tr>
                <th>CODIGO</th>
                <th>DESCRIPCION</th>
                <th>
                    ACCIONES
                    <button class="action-btn add" title="Agregar almacÃ©n">
                        <i class="fas fa-plus"></i>
                    </button>
                </th>
            </tr>
        </thead>
        <tbody>
            <% dataToSend.forEach(data => { %>
                <tr tabindex="0">
                    <td><%= data.id %></td>
                    <td><%= data.nombre %></td>
                    <td class="actions">
                        <button class="action-btn edit" title="Editar"><i class="fas fa-pencil-alt"></i></button>
                        <button class="action-btn delete" title="Eliminar"><i class="fas fa-trash"></i></button>
                        <button class="action-btn view" title="Ver"><i class="fas fa-eye"></i></button>
                    </td>
                </tr>
            <% }); %>
        </tbody>
    </table>
</div>
